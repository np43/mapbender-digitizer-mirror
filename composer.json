{
    "name": "mapbender/digitizer",
    "description": "Mapbender digitizer element",
    "keywords": ["mapbender", "digitizer","digitizing"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {"name": "Andriy Oblivantsev"},
        {"name": "Stefan Winkelmann"},
        {"name": "David Patzke"}
    ],
    "require": {
        "php": ">=5.3.3",
        "blueimp/jquery-file-upload": "9.*",
        "debugteam/bootstrap-colorpicker": "1.x",
        "mapbender/data-source": "0.*",
        "phayes/geophp": "1.2",
        "mapbender/data-manager":  "*",
        "mapbender/vis-ui.js": "*"
    },
    "minimum-stability": "dev",
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {"Mapbender\\DigitizerBundle\\": "."}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev",
            "dev-release/1.0": "1.0.x-dev"
        }
    }
